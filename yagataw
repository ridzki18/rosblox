loadstring([[
local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local function getHRP() local c = LP.Character or LP.CharacterAdded:Wait(); return c:WaitForChild("HumanoidRootPart", 5) end

-- Buat BillboardGui saja
local hrp = getHRP()
local bb = Instance.new("BillboardGui")
bb.Name = "TP_Control"
bb.Adornee = hrp
bb.Size = UDim2.fromOffset(100, 40)
bb.StudsOffset = Vector3.new(0,3,0)
bb.AlwaysOnTop = true
bb.Parent = hrp

local btn = Instance.new("TextButton", bb)
btn.Size = UDim2.fromScale(1,1)
btn.Text = "TP ON"
btn.TextScaled = true
btn.BackgroundColor3 = Color3.new(0.3, 0.6, 0.3)
btn.TextColor3 = Color3.new(1,1,1)

local active = false
btn.MouseButton1Click:Connect(function()
    active = not active
    btn.Text = active and "TP OFF" or "TP ON"
end)

-- Very basic teleport loop 1-8
task.spawn(function()
    while true do
        if active then
            for i=1,8 do
                local cp = workspace.Checkpoints:FindFirstChild(tostring(i))
                if cp and cp:IsA("BasePart") then
                    local part = cp
                    LP.Character.HumanoidRootPart.CFrame = CFrame.new(part.Position + Vector3.new(0,4,0))
                end
                task.wait(2)
            end
        end
        task.wait(0.2)
    end
end)
]])()
